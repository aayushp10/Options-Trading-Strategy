{
	"data": {
		"sequence_length": 20,
		"train_test_split": 0.85,
		"normalise": true
	},
	"training": {
		"epochs": 2,
		"batch_size": 20
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"layers": [
			{
				"type": "lstm",
				"neurons": 50,
				"input_timesteps": 20,
				"input_dim": 1,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": false
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}